# File to save custom aliases

# different cd aliases
alias wp='cd ~/Dokumente/Uni/14W/WiPro'
alias uni='cd ~/Dokumente/Uni'
alias pra='cd ~/Dokumente/Uni/14W/Praktikum3'
alias unia='cd ~/Dokumente/Uni/14W'

# quick update
alias update='sudo apt-get update && sudo apt-get upgrade'

# vim aliases
alias v='vim'
# vimrc
alias vv='vim ~/.vimrc'
# tmux.conf
alias vz='vim ~/.zshrc'
alias vt='vim ~/.tmux.conf'
# i3 config
alias vi='vim ~/.i3/config'

# tmux
alias tmux='TERM=xterm-256color tmux'
alias tp='cd ~/Dokumente/Uni/14W/WiPro && TERM=xterm-256color tmux'
alias tks='tmux kill-server'

# git
alias gitup='cp -r ~/.vim ~/.vimrc ~/.zsh ~/.zshrc ~/.tmux.conf ~/.bashrc ~/.bash_aliases ~/.i3 ~/bin ~/.vimperator ~/.vimperatorrc ~/Dokumente/git/dotfiles'
