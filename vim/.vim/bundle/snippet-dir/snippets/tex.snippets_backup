# #DOCUMENT
# # \begin{}...\end{}
# snippet beg \begin{} ... \end{} block
# 	\begin{${1:env}}
# 		${2}
# 	\end{$1}
# # Tabular
# snippet tabular tabular (or arbitrary) environment
# 	\begin{${1:tabular}}{${2:c}}
# 		${3}
# 	\end{$1}
# snippet thm thm (or arbitrary) environment with optional argument
# 	\begin[${1:author}]{${2:thm}}
# 		${3}
# 	\end{$2}
# snippet center center environment
# 	\begin{center}
# 		${1}
# 	\end{center}
# # Align(ed)
# snippet ali align(ed) environment
# 	\begin{align${1:ed}}
# 		\label{eq:${2}}
# 		${3}
# 	\end{align$1}
