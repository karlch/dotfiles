# snippet tabl add a tabular line
# 	${1} & ${2} \\\ \hline${3}
# snippet _ underscore env
# 	${1:value}_{${2:index}} ${3}
# snippet ^ upperscore env
# 	${1:base}^{${2:exponent}} ${3}
# snippet + + with spaces in math mode
# 	~+~ ${1}
# snippet - - with spaces in math mode
# 	~-~ ${1}
# snippet = = with spaces in math mode
# 	~=~ ${1}
# snippet * multiply
# 	\cdot ${1}
# snippet geq greater or equal than
# 	~\geq ~ ${1}
# snippet leq less or equal than
# 	~\leq ~ ${1}
# snippet sq square root
# 	\sqrt{${1}}
# snippet val value with error
# 	(${1:value} ~\pm ~ ${2:error})~${3:unit}
# snippet err begin an error
# 	$\Delta ${1:Fehler}~=~${2:Fehlerrechnung}$ \\\
# # snippet stat general statistical error
# # 	\Delta ${1:Fehler}^2 \cdot \left| ${2:Ableitungen} \right| ^2
# snippet sys general systematical error
# 	\Delta ${1:Fehler} \cdot \left| ${2:Ableitungen} \right|
# snippet start start complete physics document
# 	\documentclass[11pt]{article}
# 	\title{\underline{${1:title}}}
# 	\author{${2:author}}
# 	\date{\today}
#
# 	\usepackage[top=2cm,bottom=2cm,left=2cm,right=2cm]{geometry}
#
# 	\usepackage{fancyhdr}
# 	\pagestyle{fancy}
# 	\lhead{$2}
# 	\chead{${3:group}}
# 	\rhead{\today}
# 	\cfoot{\thepage}
#
# 	\usepackage[${4:language}]{babel}
#
# 	\usepackage{graphicx}
#
# 	\usepackage[utf8]{inputenc}
#
# 	\begin{document}
#
# 	\parindent0pt
# 	\enlargethispage{1.5cm}
# 	\maketitle
# 	\tableofcontents
# 	\newpage
#
# 	${5}
#
# 	\end{document}
