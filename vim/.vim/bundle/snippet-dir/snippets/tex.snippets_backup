# 	\end{itemize}
# snippet item \item
# 	\item ${1}
# # Description
# snippet desc description environment
# 	\begin{description}
# 		\item[${1}] ${2}
# 	\end{description}
# # Endless new item
# snippet ]i \item (recursive)
# 	\item ${1}
# 		${2:]i}
